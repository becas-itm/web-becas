{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jest-environment-jsdom-sixteen",
    "test:staged": "cross-env CI=true react-app-rewired test --env=jest-environment-jsdom-sixteen --bail --findRelatedTests --colors",
    "eject": "react-scripts eject",
    "build:icons": "node src/ui/components/Icon/buildIcons",
    "cypress:open": "cypress open",
    "storybook": "start-storybook -p 9009 -s public",
    "build:storybook": "build-storybook -s public",
    "lint": "eslint .storybook cypress src"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --fix",
      "yarn test:staged"
    ]
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "react-app",
      "plugin:cypress/recommended",
      "plugin:prettier/recommended",
      "prettier/react"
    ],
    "rules": {
      "prettier/prettier": "error"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@popperjs/core": "^2.3.1",
    "@reach/alert": "^0.10.3",
    "@reach/dialog": "^0.10.0",
    "@reach/listbox": "^0.10.0",
    "@reach/menu-button": "^0.10.5",
    "axios": "^0.19.2",
    "classnames": "^2.2.6",
    "date-fns": "^2.12.0",
    "downshift": "^5.1.0",
    "formik": "^2.1.4",
    "framer-motion": "^2.3.0",
    "match-sorter": "^4.1.0",
    "qs": "^6.9.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-fast-compare": "^3.0.1",
    "react-icons": "^3.10.0",
    "react-kawaii": "^0.16.0",
    "react-markdown": "^4.3.1",
    "react-popper": "^2.2.2",
    "react-query": "^1.2.1",
    "react-router-dom": "^5.2.0",
    "react-use": "^14.1.1",
    "yup": "^0.28.3"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-knobs": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addon-storysource": "^5.3.18",
    "@storybook/addon-viewport": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/preset-create-react-app": "^2.1.1",
    "@storybook/react": "^5.3.18",
    "@testing-library/cypress": "^6.0.0",
    "@testing-library/dom": "^7.21.7",
    "@testing-library/jest-dom": "^5.11.2",
    "@testing-library/react": "^10.4.7",
    "@testing-library/user-event": "^12.1.0",
    "@types/jest": "^25.2.1",
    "@types/testing-library__cypress": "^5.0.6",
    "cross-env": "^7.0.2",
    "customize-cra": "^1.0.0",
    "cypress": "^4.12.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-prettier": "^3.1.2",
    "http-proxy-middleware": "^1.0.3",
    "husky": "^4.2.5",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "lint-staged": "^10.1.5",
    "node-sass": "^7.0.0",
    "prettier": "^2.0.4",
    "prop-types": "^15.7.2",
    "react-app-rewire-multiple-entry": "^2.2.0",
    "react-app-rewired": "^2.1.6",
    "react-scripts": "3.4.1",
    "svgo": "^1.3.2",
    "tailwindcss": "^1.6.2"
  }
}
